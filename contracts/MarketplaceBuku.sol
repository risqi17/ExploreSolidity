pragma solidity >=0.8.2 <0.9.0;

contract marketplaceBuku {
    struct Buku {
        string namaBuku;
        uint hargaBuku;
    }

    Buku[] public semuaBuku;

    function tambahBuku(string _namaBuku, uint _hargaBuku) public {
        semuaBuku.
    }
}